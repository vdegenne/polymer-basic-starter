{
  "entrypoint": "index.html",
  "shell": "src/components/%appname%.js",
  "sources": [
    "images/**/*",
    "fonts/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "push-manifest.json"
  ],
  "moduleResolution": "node",
  "npm": true,
  "builds": [
    {
      "name": "es6prod",
      "preset": "es6-bundled",
      "addServiceWorker": false
    }
  ]
}